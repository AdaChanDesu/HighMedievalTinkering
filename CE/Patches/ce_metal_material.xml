<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[
                  @Name="TA_NasalBase" or
                  defName="TA_MailleVest" or
                  defName="TA_CoatOfPlates" or
                  defName="TA_Greathelm" or
                  defName="TA_KettleHat" or
                  defName="TA_CoifMaille" or
                  defName="TA_MailleHoses" or
                  defName="TA_MailleMittens"
			]/stuffCategories</xpath>
		<value>
			<stuffCategories>
				<li>Steeled</li>
			</stuffCategories>
		</value>
	</Operation>

      <Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[
            defName="TA_GreatAxe"]/stuffCategories</xpath>
		<value>
			<stuffCategories>
                <li>Metallic_Weapon</li>
			</stuffCategories>
		</value>
	</Operation>

      
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[@ParentName="BaseMeleeWeapon_Sharp" or @ParentName="BaseMeleeWeapon_Blunt" or @ParentName="BaseMeleeWeapon_Sharp_Quality" or @ParentName="BaseMeleeWeapon_Blunt_Quality"]/stuffCategories/li[.="Metallic"]</xpath>
		<value>
			<li>Metallic_Weapon</li>
		</value>
	</Operation>
      
</Patch>